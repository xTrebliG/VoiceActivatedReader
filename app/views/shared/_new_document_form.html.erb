<%= form_for [@user,@document], url: documents_path, html: { multipart: true } do |f| %>
    <h1>Upload Your PDF!</h1>
    <%= f.text_field :title, placeholder: 'Title' %> <br/>
    <%= f.text_area :description, placeholder: 'Description' %> <br/>
    <%= f.file_field :pdf %> <br/>
    <%= f.submit %>


<% end %>